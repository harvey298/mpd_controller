{
  "schemaVersion": 1,
  "id": "mpd_controller",
  "version": "${version}",
  "name": "mpd_controller",
  "description": "A mod to control MPD",
  "authors": [
    "hoovy"
  ],
  "contact": {
    "website": "https://hoovy.co.uk/mods/minecraft",
    "repo": "https://github.com/harvey298/mpd_controller"
  },
  "license": "GPL-3.0",
  "icon": "assets/mpd_controller/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "uk.co.hoovy.mpd_controller.Mpd_controllerClient"
    ],
    "main": [
      "uk.co.hoovy.mpd_controller.Mpd_controller"
    ]
  },
  "mixins": [
    "mpd_controller.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "custom": {
